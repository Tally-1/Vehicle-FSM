                         "[_NearbyObjects, _HigherThanEn, _LOSrank]};" \n
                         "Tally_Fnc_InitRepairs = {" \n
                         "params[""_Vehicle""];" \n
                         "private _Crew 	= (_Vehicle GetVariable ""Crew"");" \n
