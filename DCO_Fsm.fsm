                         "" \n
                         "" \n
                         "Private _MBTparents = [	""MBT_03_base_F""," \n
                         "						""MBT_04_base_F""," \n
                         "						""O_MBT_02_base_F""," \n
                         "						""B_MBT_01_base_F""," \n
                         "						""MBT_02_base_F""," \n
                         "						""MBT_01_base_F""];" \n
                         "" \n
                         "Private _APCparents = [""APC_Wheeled_03_base_F""," \n
